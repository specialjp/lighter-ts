{
  "name": "lighter-ts-sdk",
  "version": "1.0.1",
  "description": "TypeScript SDK for Lighter Protocol Unofficial - Trade perpetuals with unmatched efficiency and fairness",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "wasm",
    "examples",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:wasm": "cd temp-lighter-go && go build -o ../wasm/lighter-signer.wasm -target wasm ./wasm",
    "build:wasm:nodejs": "cd temp-lighter-go && GOOS=js GOARCH=wasm go build -o ../wasm/lighter-signer.wasm ./wasm",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src --out docs/api",
    "examples": "ts-node examples/get_info.ts"
  },
  "keywords": [
    "lighter",
    "protocol",
    "typescript",
    "sdk",
    "defi",
    "perpetual",
    "exchange",
    "zkwasm",
    "wasm"
  ],
  "author": "Community Contributor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bvvvp009/lighter-ts.git"
  },
  "bugs": {
    "url": "https://github.com/bvvvp009/lighter-ts/issues"
  },
  "homepage": "https://lighter.xyz",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^17.2.0",
    "ethers": "^6.8.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}